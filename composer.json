{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "deployer/deployer": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.8",
        "sensiolabs/security-checker": "^4.1",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^3.4",
        "symfony/maker-bundle": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "",
            "allow-contrib": false
        }
    }
}